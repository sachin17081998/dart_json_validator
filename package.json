{
    "name": "dart-model-tester",
    "displayName": "DMT:Fart Model Tester",
    "publisher": "Aachi Kumar Singh",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.60.0"
    },
    "main": "./out/extensions.js",
    "activationEvents": [
        "onStartupFinished",
        "onCommand:dartJsonValidator.runParse"
    ],
    "contributes": {
        "commands": [
            {
                "command": "dartJsonValidator.runParse",
                "title": "DMT: Test Dart Model with JSON"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -w -p ./",
        "pretest": "npm run compile",
        "test": "mocha -r ts-node/register test/**/*.spec.ts"
    },
    "devDependencies": {
        "@types/node": "^16.18.126",
        "chai": "^4.0.0",
        "mocha": "^9.0.0",
        "ts-node": "^10.0.0",
        "typescript": "^4.0.0",
        "vscode": "^1.1.36"
    }
}